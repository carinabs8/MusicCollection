<h1><%= t(:artists) %></h1>
<div class="row">
	<% @artists.each do |_key, artist| %>
		<div class="card" style="width: 18rem;">
		  <div class="card-body">
		    <h5 class="card-title"><%= artist["name"] %></h5>
		    <h6 class="card-subtitle mb-2 text-muted"><%= artist["twitter"] %></h6>
		  </div>
		</div>
	<% end %>
</div>